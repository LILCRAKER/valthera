# AWS Cognito Configuration
# These will be automatically set by CDK during deployment
VITE_COGNITO_USER_POOL_ID=us-east-1_your_user_pool_id_here
VITE_COGNITO_USER_POOL_CLIENT_ID=your_user_pool_client_id_here
VITE_COGNITO_IDENTITY_POOL_ID=us-east-1:your_identity_pool_id_here

# API Configuration
# Production API Gateway URL (from CDK)
VITE_API_BASE_URL=https://your-api-gateway-url.amazonaws.com/prod
# Development/local API URL
# VITE_API_BASE_URL=http://localhost:8000

# API Gateway Key (get this from CDK outputs after deployment)
VITE_API_KEY=your_api_gateway_key_here

# Environment
NODE_ENV=production

# AWS Region
VITE_AWS_REGION=us-east-1

# Cognito Domain (if using hosted UI)
VITE_COGNITO_DOMAIN=your-cognito-domain.auth.us-east-1.amazoncognito.com

# Additional API Configuration
VITE_API_GATEWAY_URL=https://f6hjr3eve7.execute-api.us-east-1.amazonaws.com/dev 